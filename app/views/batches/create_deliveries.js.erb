$('#deliveries').html('<% @deliveries.reverse.each do |b| %><div id="delivery<%= b.id %>" class="col-md-12 table-content delivery-content" address="<%= b.recipient_address %>" phone="<%= b.recipient_phone_number %>" name="<%= b.id %>"><div class="row"><div class="col-md-3 no-right-padding" ><span><%= b.recipient_name %></span></div><div class="col-md-4 no-horizontal-padding"><span><%= b.recipient_address %></span></div><div class="col-md-2 no-horizontal-padding"><span><%= b.recipient_phone_number %></span></div><div class="col-md-1 no-horizontal-padding"><%= b.copay %></div><div class="col-md-1 no-horizontal-padding"><%= b.time %></div><div class="col-md-1 no-horizontal-padding"><%= link_to remove_delivery_path(:id => b.id), class:"btn btn-warning removeDelivery", remote: true, method: :get do %><i class="fa fa-times-circle font-10 theme-blue"></i><% end %></div></div></div><% end %>');
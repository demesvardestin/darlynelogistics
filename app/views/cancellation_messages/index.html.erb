<p id="notice"><%= notice %></p>

<h1>Listing Cancellation Messages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cancellation_messages.each do |cancellation_message| %>
      <tr>
        <td><%= link_to 'Show', cancellation_message %></td>
        <td><%= link_to 'Edit', edit_cancellation_message_path(cancellation_message) %></td>
        <td><%= link_to 'Destroy', cancellation_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cancellation message', new_cancellation_message_path %>
